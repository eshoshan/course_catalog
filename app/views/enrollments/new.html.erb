<h1>Courses Enrolled for <%=current_user.name%></h1>

<% e = Enrollment.where(user_email: current_user.email) %>

<% for ele in e %>
  <% a = Course.where(code: ele.course_code) %>
  <div class = "row">
    <div class="col-md-2">
    </div>
    <div class="col-md-3">
      <%= ele.course_code %>
    </div>
    <div class="col-md-6">
      <% final = [] %>
      <% temp = Course.where(code: ele.course_code)%>
      <% temp.each { |t| final.append(t.name)} %>
      <%= final.to_s %>
    </div>

    <div class="col-md-1">
    </div>
    <br>
  </div>
<% end %>
